	"version": "0.2.0",
	"configurations": [

	{
		"type": "node",
		"request": "launch",
		"name": "Jest Tests",
		"program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
		"args": [
			"-i"
		]
	},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach",
			"port": 5858,
			"processId": "${command:PickProcess}"
		}
	]
}
